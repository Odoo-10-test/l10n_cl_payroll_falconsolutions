<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<data>
     <record id="l10n_cl_payroll_hr_salary_rule_form" model="ir.ui.view">
            <field name="name">l10n_cl_payroll.hr_salary_rule.form</field>
            <field name="model">hr.salary.rule</field>
            <field name="inherit_id" ref="hr_payroll.hr_salary_rule_form" />
            <field name="arch" type="xml">
                 <page name="rules" position="before" >
                    <page name="public" string="RR.HH Chile">
                        <sheet>
                            <group>
                                <group string="Dates">
                                    <field name="start_date"/>
                                    <field name="end_date"/>
                                </group>
                            </group>
                        </sheet>
                 </page>
                </page>
            </field>
        </record>
</data>
</odoo>